{
  "name": "cspace-cli",
  "version": "0.0.1",
  "description": "CollectionSpace CLI",
  "author": "Ray Lee <ray.lee@lyrasis.org>",
  "license": "ECL-2.0",
  "keywords": [
    "collectionspace",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ray-lee/cspace-cli.js.git"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "bin": {
    "cspace": "bin/cspace"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=cjs babel ./src -d lib",
    "check": "npm run lint && npm run test && npm run coverage",
    "clean": "rimraf lib coverage",
    "coverage": "istanbul report --root coverage text && istanbul report --root coverage lcov",
    "lint": "eslint src",
    "prepare": "npm run clean && npm run build",
    "test": "cross-env NODE_ENV=test nyc mocha --require @babel/register --recursive test"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.0",
    "babel-plugin-istanbul": "^5.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "cspace-api": "^1.0.0",
    "get-stdin": "^6.0.0",
    "inquirer": "^6.2.2",
    "lodash": "^4.17.11",
    "netrc": "^0.1.4",
    "progress": "^2.0.3",
    "require-from-string": "^2.0.2",
    "yargs": "^12.0.2"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "json"
    ],
    "report-dir": "./coverage/Node.js",
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
